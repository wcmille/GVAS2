int NumParse(string argument, string trigger) {
  int output=0;
  argument=TL(argument);
  trigger=TL(trigger);
  int.TryParse(argument.Substring(argument.IndexOf(trigger) + trigger.Length).Trim(), out output);
  return Math.Max(output,0);
}

