            Displayname = block.CustomName; // for Error reporting
            string temp_txt = block.CustomData.Substring(block.CustomData.IndexOf(lcdtag) + lcdtag.Length).Trim();
            string[] sections = ("\n" + temp_txt).Split(new[] { "\nPanel ", "\npanel ", "\nPANEL " }, StringSplitOptions.None).Where(x =&gt; !string.IsNullOrEmpty(x)).Select(s =&gt; s.Trim()).ToArray();
            for (int i = 0; i &lt; sections.Length; i++)
            {
                string[] lines = sections[i].Split('\n').Where(x =&gt; !string.IsNullOrEmpty(x)).ToArray();
                if (int.TryParse(lines.First(), out surfaceIndex))
                {       //get surface index
                    if (surfaceIndex &lt; provider.SurfaceCount)
                    {     //surface exists?
                        surface = provider.GetSurface(surfaceIndex);
                        sArgs = lines.Skip(1).ToArray();                      //get what to show on screen
                    }
                }
                else
                {  //no surface index found
                    surface = provider.GetSurface(0);
                    sArgs = lines;                      //get what to show on screen
                }
                DrawScreen(surface, argument);
                screenIndex++;
            }
        }
    }
    switch (argument)
    {
        case "shutdown":
            Runtime.UpdateFrequency = UpdateFrequency.None;
            Echo("             &lt;&lt; Notice &gt;&gt;");
            Echo("This F.S.D. entity is shut down.");
            Echo("Run this Programming Block");
            Echo("with the argument " + '"' + "powerup" + '"');
            Echo("to restart it again.");
            execCounter = 0;
            break;
        case "powerup":
            Runtime.UpdateFrequency = UpdateFrequency.Update100;
            Echo(" ");
            execCounter = 0;
            break;
        case "refresh":
            Runtime.UpdateFrequency = UpdateFrequency.Update100;
            Echo("refreshing");
            execCounter = 0;
            break;
        case "":
            Echo("   &lt;&lt; F.S.D. is active &gt;&gt;");
            Echo("This Programming Block is");
            Echo("handling " + plSing("LCD Panel", screenIndex) + " at the");
            Echo("moment.");
            break;
    }
}
public void DetectIgnoredGrids()
{
    List&lt;IMyProgrammableBlock&gt; pbs = new List&lt;IMyProgrammableBlock&gt;();
    ignoredGrids.Clear();
    if (!Me.CustomName.EndsWith("[FSD]")) { Me.CustomName += " [FSD]"; }
    GridTerminalSystem.GetBlocksOfType&lt;IMyProgrammableBlock&gt;(pbs, p =&gt; p.CustomName.Contains("[FSD]") &amp;&amp; !(p.CubeGrid == Me.CubeGrid));
    foreach (var pb in pbs) { ignoredGrids.Add(pb.CubeGrid); }
}
public void DrawScreen(IMyTextSurface surface, string argument)
{
    viewport = new RectangleF((surface.TextureSize - surface.SurfaceSize) / 2f, surface.SurfaceSize);   //draw area
    pos = LineStartpos = viewport.Position + new Vector2(7, 0);  //draw starting positon for each screen
    frame = surface.DrawFrame();
    string[] groupAndOptions;
    string c_arg;
    bool continueDrawing = true;
    int lastDraw = Nothing;
    int optionpos = 0;
    int texttagpos = 0;
    headlineIndex = 0;
    //Set up draw surface
    PrepareTextSurface(surface);
    if (argument == "shutdown" || argument == "refresh")
    {
        AddTextSprite(frame, pos, " ", "DEBUG", 1f, Color.Black, TextAlignment.LEFT);
    }
    else
    {
        //AddTextureSprite(frame, background, viewport.Center, viewport.Size);  //Add Background?
        foreach (var arg in sArgs)
        {// get what to draw on a specific screen
            LineStartpos.X = viewport.X + 7;   //reset x after used positiontag #rc
            optionpos = arg.IndexOfAny(new char[] { ',', ':' });
            if (optionpos &gt;= 0)
            {
                c_arg = arg.ToLower().Substring(optionpos); // separating the options
                texttagpos = c_arg.IndexOf(texttag.ToLower());
                if (texttagpos &gt;= 0)
                { // looking for texttag
                    added_txt = arg.Substring(texttagpos + texttag.Length + optionpos);
                    c_arg = c_arg.Remove(texttagpos);
                }
                else
                {
                    added_txt = "";
                }
                if (c_arg.Contains(positiontag.ToLower()))
                { // looking for positiontag
                    Vector2 tempvector;
                    if (getVector2(c_arg, out tempvector))
                    {
                        pos = LineStartpos = tempvector;
                    }
                }
                optionalView = c_arg.Contains(optionaltag.ToLower()); // looking for optionaltag
                noheadline = c_arg.Contains(noheadlinetag.ToLower()); // looking for noheadlinetag
                nolinebreak = c_arg.Contains(nolinebreaktag.ToLower()); // looking for nolinebreaktag
                noscrolling = c_arg.Contains(noscrolltag.ToLower());
                if (c_arg.Contains(widebartag.ToLower()))
                { // looking for widebartag
                    if (viewport.Width &lt; 500) { containertype = SmallBar; } else { containertype = WideBar; }
                }
                else if (c_arg.Contains(smallbartag.ToLower()))
                { // looking for smallbartag
                    containertype = SmallBar;
                }
                else
                {
                    containertype = SingleContainer;
                }
                c_arg = arg.Substring(0, optionpos);
            }
            else
            {
                optionalView = false;
                noheadline = false;
                nolinebreak = false;
                noscrolling = false;
                containertype = SingleContainer;
                added_txt = "";
                c_arg = arg;
            }
            groupAndOptions = c_arg.Trim().Split(new char[] { '+' }, StringSplitOptions.RemoveEmptyEntries);
            if (continueDrawing)
            {
                continueDrawing = CycleDrawBlocks(groupAndOptions, lastDraw);     //draw blocks or blockgroups
                lastDraw = containertype;
            }
        }
    }
    frame.Dispose();
}
public bool CycleDrawBlocks(string[] groupOrBlocknames, int lastDraw)
{
    Vector2 posHeadline = new Vector2(0, 0);
    Vector2 posContainerGroup = posHeadline;
    StringBuilder headline = new StringBuilder();
    float percent = 0;
    bool continueDrawing = true;
    ClearGroupArrays();
    Group.Clear();
    switch (lastDraw)
    {
        case WideBar:
            if ((pos.X + 508) &gt;= viewport.Width || containertype != lastDraw) { LineStartpos.Y += 72; pos = LineStartpos; }
            break;
        case SmallBar:
            if ((pos.X + 256) &gt;= viewport.Width || containertype != lastDraw) { LineStartpos.Y += 55; pos = LineStartpos; }
            break;
        case SingleContainer:
            LineStartpos.Y += 95; pos = LineStartpos;
            break;
        case Textline:
            LineStartpos.Y += 23; pos = LineStartpos;
            break;
    }
    if ((!noheadline || added_txt != "") &amp;&amp; containertype == SingleContainer)
    {
        LineStartpos.Y += 23; pos = LineStartpos;  // leave room for drawing headline over containers
        posContainerGroup = pos;
    }
    foreach (string entry in groupOrBlocknames)
    {
        var foundgroup = GridTerminalSystem.GetBlockGroupWithName(entry);     //search either for blockgroup
        if (foundgroup != null)
        {
            if (groupOrBlocknames.Length == 1 &amp;&amp; added_txt == "")
            {
                headline.Append(foundgroup.Name + ": ");
            }
            foundgroup.GetBlocks(Group);
        }
        else
        {
            GridTerminalSystem.SearchBlocksOfName(entry, Group);   //or for blocks with name
        }
        if (entry.ToLower().Contains(gaptag.ToLower()))
        {
            int gap = 0;
            if (int.TryParse(entry.ToLower().Replace(gaptag.ToLower(), ""), out gap))
            {
                pos.X += gap;
                Group.Clear();
            }
        }
        else if (Group.Count &lt; 1)
        {
            Echo("         &lt;&lt; F.S.D. Warning &gt;&gt;");
            Echo("No Group or Block with the Name:");
            Echo('"' + entry + '"');
            Echo("found as listed in the ToDo List of:");
            Echo('"' + Displayname + '"');
        }
        bool continuegroup = true;
        foreach (var block in Group)
        {       //loop through Group
            if (GetBlockProperties(block, ref percent))
            {
                if (continuegroup &amp;&amp; containertype == SingleContainer)
                {
                    if ((pos.X + 72) &gt; viewport.Width)
                    {   //test if item fits in line
                        if (!nolinebreak)
                        {
                            if ((pos.Y + 170) &gt; viewport.Bottom)
                            {   //test if item fits in next line
                                continuegroup = false;
                            }
                            else
                            {
                                LineStartpos.Y += 95;   //linebreak
                                pos = LineStartpos;
                            }
                        }
                        else
                        {
                            continuegroup = false;
                        }
                    }
                    if (continuegroup)
                    {
                        GetChargeStateValues(percent, SingleContainer);
                        DrawSingleContainer(pos, frameColor);   //draw block
                        pos.X += 72;   //position for next item in line
                    }
                }
            }
        }
    }
    if (sumAll[0] == 0 &amp;&amp; added_txt != "")
    {
        if (containertype == SingleContainer) { LineStartpos.Y -= 23; pos = LineStartpos; }
        containertype = Textline;
    }
    else if (sumAll[0] &gt; 0)
    {
        percent = (int)Math.Truncate(sumAll[1] / sumAll[0]);
    }
    switch (containertype)
    {
        case WideBar:
            GetChargeStateValues(percent, WideBar);
            posHeadline = pos + new Vector2(15, 14);  //place headline inside wide bar
            DrawCargebarWide(pos, Color.White);
            pos.X += 508;
            if ((pos.Y + 0) &gt; viewport.Height) { continueDrawing = false; }  // end of screen
            break;
        case SmallBar:
            GetChargeStateValues(percent, SmallBar);
            posHeadline = pos + new Vector2(15, 14);  //place headline inside wide bar
            DrawCargebarWide(pos, Color.White);
            pos.X += 256;
            if ((pos.Y + 0) &gt; viewport.Height) { continueDrawing = false; }  // end of screen
            break;
        case SingleContainer:
            posHeadline = posContainerGroup + new Vector2(0, -23);  //place headline above containers
            if ((pos.Y) &gt; viewport.Height) { continueDrawing = false; }  // end of screen
            break;
        case Textline:
            posHeadline = LineStartpos;
            if ((pos.Y + 23) &gt;= viewport.Height) { continueDrawing = false; }  // end of screen
            break;
    }
    headline.Append(added_txt);
    if (!noheadline)
    { //   build headline from collected arrays
        if (sumBat[0] &gt; 0)
        {
            headline.Append(SepStart + plSing(" Battery", sumBat[0]) + ": ");
            if (optionalView) { headline.Append("In " + TruncateUnit(sumBat[1], "watt") + "Out "); }
            else { headline.Append(f_percent(sumBat)); }
            headline.Append(TruncateUnit(sumBat[2], "watt") + SepStop);
        }
        if (sumReactor[0] &gt; 0)
        {
            headline.Append(SepStart + plSing(" Reactor", sumReactor[0]) + ": ");
            if (optionalView) { headline.Append(TruncateUnit(sumReactor[2], "kg") + "Uranium " + SepStop); } else { headline.Append("Output " + Math.Truncate(sumReactor[1] / sumReactor[0]) + "% " + TruncateUnit(sumReactor[2], "watt") + SepStop); }
        }
        if (sumGasgen[0] &gt; 0)
        {
            headline.Append(SepStart + plSing(" Hydrogen Engine", sumGasgen[0]) + ": ");
            if (optionalView) { headline.Append("Output " + f_percent(sumGasgen) + TruncateUnit(sumGasgen[2], "watt") + SepStop); } else { headline.Append(f_percent(sumGasgen) + TruncateUnit(sumGasgen[2], "lit") + SepStop); }
        }
        if (sumJdrives[0] &gt; 0) { headline.Append(SepStart + plSing("Jumpdrive", sumJdrives[0]) + ": " + f_percent(sumJdrives) + TruncateUnit(sumJdrives[2], "whr") + SepStop); }
        if (sumCargo[0] &gt; 0) { headline.Append(SepStart + plSing(" Cargo", sumCargo[0]) + ": " + f_percent(sumCargo) + TruncateUnit(sumCargo[2], "c_l")); }
        if (sumConnectors[0] &gt; 0) { headline.Append(SepStart + plSing(" Connector", sumConnectors[0]) + ": " + f_percent(sumConnectors) + TruncateUnit(sumConnectors[2], "c_l") + SepStop); }
        if (sumEjectors[0] &gt; 0) { headline.Append(SepStart + plSing(" Ejector", sumEjectors[0]) + ": " + f_percent(sumEjectors) + TruncateUnit(sumEjectors[2], "c_l") + SepStop); }
        if (sumDrills[0] &gt; 0) { headline.Append(SepStart + plSing(" Drill", sumDrills[0]) + ": " + f_percent(sumDrills) + TruncateUnit(sumDrills[2], "c_l")); }
        if (sumHydro[0] &gt; 0) { headline.Append(SepStart + plSing(" Hydrogen Tank", sumHydro[0]) + ": " + f_percent(sumHydro) + TruncateUnit(sumHydro[2], "lit") + SepStop); }
        if (sumOxy[0] &gt; 0) { headline.Append(SepStart + plSing(" Oxygen Tank", sumOxy[0]) + ": " + f_percent(sumOxy) + TruncateUnit(sumOxy[2], "lit") + SepStop); }
        if (sumEShield[0] &gt; 0) { headline.Append(SepStart + plSing(" Shield Generator", sumEShield[0]) + ": " + f_percent(sumEShield) + "Required Input: " + eShieldInput + " " + SepStop); }
        if (sumShield[0] &gt; 0) { headline.Append(SepStart + plSing(" Shield Controller", sumShield[0]) + ": " + Math.Truncate(sumShield[1]) + "% " + "Overheated: " + Math.Truncate(sumShield[2]) + "% " + SepStop); }
        if (sumMissileTurret[0] &gt; 0) { headline.Append(SepStart + plSing(" Missile Turret", sumMissileTurret[0]) + ": " + f_percent(sumMissileTurret) + plSing(" Missile", sumMissileTurret[2]) + " " + SepStop); }
        if (sumGatlingTurret[0] &gt; 0) { headline.Append(SepStart + plSing(" Gatling Turret", sumGatlingTurret[0]) + ": " + f_percent(sumGatlingTurret) + plSing(" Unit", sumGatlingTurret[2]) + " " + SepStop); }
        if (sumMissileLauncher[0] &gt; 0) { headline.Append(SepStart + plSing(" Rocket Launcher", sumMissileLauncher[0]) + ": " + f_percent(sumMissileLauncher) + plSing(" Rocket", sumMissileLauncher[2]) + " " + SepStop); }
        if (sumGatling[0] &gt; 0) { headline.Append(SepStart + plSing(" Gatling Gun", sumGatling[0]) + ": " + f_percent(sumGatling) + plSing(" Unit", sumGatling[2]) + " " + SepStop); }
        if (sumThrust[0] &gt; 0) { headline.Append(SepStart + plSing(" Thruster", sumThrust[0]) + ": " + f_percent(sumThrust) + "Override: " + Math.Truncate(sumThrust[2] * 100 / sumThrust[0]) + "% " + SepStop); }
        if (sumSolar[0] &gt; 0) { headline.Append(SepStart + plSing(" Solar Panel", sumSolar[0]) + ": Output " + f_percent(sumSolar) + TruncateUnit(sumSolar[2], "watt") + SepStop); }
        if (sumWindmill[0] &gt; 0) { headline.Append(SepStart + plSing(" Wind Turbine", sumWindmill[0]) + ": Output " + f_percent(sumWindmill) + TruncateUnit(sumWindmill[2], "watt") + SepStop); }
        if (sumO2Gen[0] &gt; 0) { headline.Append(SepStart + plSing(" O2H2 Generator", sumO2Gen[0]) + ": " + f_percent(sumO2Gen) + TruncateUnit(sumO2Gen[2], "c_l") + SepStop); }
    }
    DrawHeadline(posHeadline, headline);   //draw headline
    return continueDrawing;
}
public void DrawHeadline(Vector2 drawposHeadline, StringBuilder headline)
{
    string headline_short = "";
    int scrollindex = scrollpositions[screenIndex, headlineIndex];
    if (headline.Length + 16 &lt; scrollindex || noscrolling) { scrollindex = 0; }
    switch (containertype)
    {
        case WideBar:
            if (trimmed_SB(scrollindex, headline, 440f, 1.3f, out headline_short)) { scrollindex++; }
            AddTextSprite(frame, drawposHeadline, headline_short, "DEBUG", 1.3f, headlineColorWideBar, TextAlignment.LEFT);
            break;
        case SmallBar:
            if (trimmed_SB(scrollindex, headline, 190f, 0.8f, out headline_short)) { scrollindex++; }
            AddTextSprite(frame, drawposHeadline, headline_short, "DEBUG", 0.8f, headlineColorWideBar, TextAlignment.LEFT);
            break;
        case SingleContainer:
            if (trimmed_SB(scrollindex, headline, viewport.Width - 25 - drawposHeadline.X, 0.8f, out headline_short)) { scrollindex++; }
            AddTextSprite(frame, drawposHeadline, headline_short, "DEBUG", 0.8f, headlineColorWideBar, TextAlignment.LEFT);
            break;
        case Textline:
            if (trimmed_SB(scrollindex, headline, viewport.Width - 25 - drawposHeadline.X, 0.8f, out headline_short)) { scrollindex++; }
            AddTextSprite(frame, drawposHeadline, headline_short, "DEBUG", 0.8f, headlineColor, TextAlignment.LEFT);
            break;
    }
    scrollpositions[screenIndex, headlineIndex] = scrollindex;
    headline.Clear();
    headlineIndex++;
}
public bool trimmed_SB(int frontcut, StringBuilder headline_org, float field_width, float font_size, out string cutted_str)
{
    if (surface.MeasureStringInPixels(headline_org, "Debug", font_size).X &lt; field_width &amp;&amp; frontcut == 0)
    {
        cutted_str = headline_org.ToString();
        return false;
    }
    else
    {
        StringBuilder headline_long = new StringBuilder("W");
        float temp_w = surface.MeasureStringInPixels(headline_long, "Debug", font_size).X;
        headline_long.Clear();
        headline_long = headline_org;
        headline_long.Append(' ', 20).Append(headline_org).Remove(0, frontcut);
        float temp_f = 2f;
        while (temp_f &gt; 1f)
        {
            temp_f = (surface.MeasureStringInPixels(headline_long, "Debug", font_size).X - field_width) / temp_w;
            headline_long.Length -= (int)temp_f;
        }
        cutted_str = headline_long.ToString();
        return true;
    }
}
public string plSing(string str, float number)
{
    if (number != 1)
    {
        if (str == "Battery") { return number + " Batteries"; }
        else { return number + " " + str + "s"; }
    }
    else { return number + " " + str; }
}
public string TruncateUnit(float number, string unit)
{
    string str = "";
    if (unit == "whr") { str = calculateunits(number, " T", " G", " M", " K", 85.932966f) + "Wh"; }
    if (unit == "lit") { str = calculateunits(number / 1000, "G ", "M ", "K ", " ", 85.932966f) + "L"; }
    if (unit == "c_l") { str = calculateunits(number, "G ", "M ", "K ", " ", 85.932966f) + "L"; }
    if (unit == "watt") { str = calculateunits(number, " T", " G", " M", " K", 85.932966f) + "W"; }
    if (unit == "kg") { str = calculateunits(number, "K ton", " ton", " Kg", " g", 107.10487f); }  //#rc
    return str + "  ";
}
public string calculateunits(float number, string sign1, string sign2, string sign3, string sign4, float width)
{
    StringBuilder sb = new StringBuilder();
    if (number &gt;= 1000000) { sb.Append(Math.Round((number / 1000000f), 2) + sign1); }
    else if (number &gt;= 1000) { sb.Append(Math.Round((number / 1000f), 2) + sign2); }
    else if (number &gt;= 1) { sb.Append(Math.Round(number, 2) + sign3); }
    else { sb.Append(Math.Round((number * 1000f), 2) + sign4); }
    float temp_f = ((width - surface.MeasureStringInPixels(sb, "Debug", 0.8f).X) / 4.981622f); // + 0.5f;
    sb.Insert(0, "                     ".Substring(0, (int)temp_f));
    return sb.ToString();
}
public string f_percent(float[] werte)
{
    StringBuilder sb = new StringBuilder();
    sb.Append(Math.Round(werte[1] / werte[0], 0) + "% ");
    float temp_f = ((49.193516f - surface.MeasureStringInPixels(sb, "Debug", 0.8f).X) / 4.981622f); // + 0.5f;
    sb.Insert(0, "                     ".Substring(0, (int)temp_f));
    //  "100% "=49.193516
    //  " "=4.981622f
    return sb.ToString();
}
public bool GetBlockProperties(IMyTerminalBlock block, ref float percent)
{
    isMonoicon = false;
    invertColor = false;
    bool isValidBlock = false;
    if (block is IMyBatteryBlock)
    {
        var bat = block as IMyBatteryBlock;
        sumBat[0]++; sumAll[0]++;
        percent = 100 / bat.MaxStoredPower * bat.CurrentStoredPower;
        sumAll[1] += percent;
        if (optionalView)
        {
            sumBat[1] += bat.CurrentInput;
            sumBat[2] += bat.CurrentOutput;
        }
        else
        {
            sumBat[1] += percent;
            sumBat[2] += bat.CurrentStoredPower;
        }
        icon = "IconEnergy";
        pattern = patternBattery;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMyCargoContainer)
    {
        CargoVolume2Arr(block, sumCargo, ref percent);
        icon = iconCargo;
        isMonoicon = true;
        pattern = patternCargo;
        invertColor = true;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMyGasTank)
    {
        var tank = block as IMyGasTank;
        if (block.BlockDefinition.SubtypeName.Contains("Hydro"))
        {
            sumHydro[0]++; sumAll[0]++;
            percent = (float)Math.Round(tank.FilledRatio * 100);
            sumHydro[1] += percent; sumAll[1] += percent;
            sumHydro[2] += (float)tank.Capacity * (float)tank.FilledRatio;
            icon = "IconHydrogen";
        }
        else
        {
            sumOxy[0]++; sumAll[0]++;
            percent = (float)Math.Round(tank.FilledRatio * 100);
            sumOxy[1] += percent; sumAll[1] += percent;
            sumOxy[2] += (float)tank.Capacity * (float)tank.FilledRatio;
            icon = "IconOxygen";
        }
        pattern = patternTank;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMyShipConnector)
    {
        if (block.BlockDefinition.SubtypeName.Contains("Large") || block.BlockDefinition.SubtypeName.Contains("Small"))
        { // Ejector: ConnectorLarge_SEBuilder4, ConnectorSmall
            CargoVolume2Arr(block, sumEjectors, ref percent);
            icon = iconEjector;
        }
        else
        { // Connector
            CargoVolume2Arr(block, sumConnectors, ref percent);
            icon = iconConnector;
        }
        isMonoicon = true;
        pattern = patternConnector;
        invertColor = true;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMyShipDrill)
    {
        CargoVolume2Arr(block, sumDrills, ref percent);
        icon = iconDrill;
        isMonoicon = true;
        pattern = patternDrill;
        invertColor = true;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMyJumpDrive)
    {
        var jumpdrive = block as IMyJumpDrive;
        sumJdrives[0]++; sumAll[0]++;
        percent = 100 / jumpdrive.MaxStoredPower * jumpdrive.CurrentStoredPower;
        sumJdrives[1] += percent; sumAll[1] += percent;
        sumJdrives[2] += jumpdrive.CurrentStoredPower;
        icon = iconJumpdrive;
        isMonoicon = true;
        pattern = patternJumpdrive;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block.GetType().ToString().Contains("MyHydrogenEngine"))
    {
        if (optionalView)
        {
            PowerOutput2Arr(block, sumGasgen, ref percent);
            icon = "IconEnergy";
        }
        else
        {
            sumGasgen[0]++; sumAll[0]++;
            percent = 100 / GasGen.MaxVolume(block) * GasGen.CurrentVolume(block);
            sumGasgen[1] += percent; sumAll[1] += percent;
            sumGasgen[2] += GasGen.CurrentVolume(block);
            icon = "IconHydrogen";
        }
        pattern = patternGasgen;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block.BlockDefinition.SubtypeName.Contains("DSControl"))
    {
        sumShield[0]++; sumAll[0]++;
        float.TryParse(getBetween(block.CustomInfo, " (", "%)"), out percent);
        sumShield[1] = percent; sumAll[1] += percent;
        float.TryParse(getBetween(block.CustomInfo, "[Over Heated]: ", "%"), out sumShield[2]);
        icon = iconShield;
        isMonoicon = true;
        pattern = patternShield;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block.BlockDefinition.SubtypeName.Contains("ShieldGenerator"))
    {
        if (!block.CustomName.Contains(":")) { block.CustomName = block.CustomName + ":"; }
        sumEShield[0]++; sumAll[0]++;
        percent = TestForNaN(100 / EnergyShield.MaxHitpoints(block) * EnergyShield.CurrentHitpoints(block));
        sumEShield[1] = percent; sumAll[1] += percent;
        eShieldInput = EnergyShield.RequiredInput(block);
        icon = iconShield;
        isMonoicon = true;
        pattern = patternShield;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMyLargeGatlingTurret || block is IMyLargeInteriorTurret)
    {
        ItemCount2Arr(block, sumGatlingTurret, ref percent);
        icon = iconBullet;
        isMonoicon = true;
        pattern = patternTurret;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMyLargeMissileTurret)
    {
        ItemCount2Arr(block, sumMissileTurret, ref percent);
        icon = iconRocket;
        isMonoicon = true;
        pattern = patternTurret;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMySmallMissileLauncher || block is IMySmallMissileLauncherReload)
    {
        ItemCount2Arr(block, sumMissileLauncher, ref percent);
        icon = iconRocket;
        isMonoicon = true;
        pattern = patternTurret;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMySmallGatlingGun)
    {
        ItemCount2Arr(block, sumGatling, ref percent);
        icon = iconBullet;
        isMonoicon = true;
        pattern = patternTurret;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMyThrust)
    {
        var thruster = block as IMyThrust;
        sumThrust[0]++; sumAll[0]++;
        percent = 100 / thruster.MaxEffectiveThrust * thruster.CurrentThrust;
        sumThrust[1] += percent; sumAll[1] += percent;
        sumThrust[2] += thruster.ThrustOverridePercentage;
        icon = iconThruster;
        isMonoicon = true;
        pattern = patternThruster;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMyReactor)
    {
        if (optionalView)
        {
            ItemCount2Arr(block, sumReactor, ref percent);
        }
        else
        {
            invertColor = true;
            PowerOutput2Arr(block, sumReactor, ref percent);
        }
        icon = iconReactor;
        isMonoicon = true;
        pattern = patternReactor;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMySolarPanel)
    {
        PowerOutput2Arr(block, sumSolar, ref percent);
        icon = iconSolar;
        isMonoicon = true;
        pattern = patternSolar;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block.BlockDefinition.SubtypeName.Contains("WindTurbine"))
    {
        PowerOutput2Arr(block, sumWindmill, ref percent);
        icon = iconWindmill;
        isMonoicon = true;
        pattern = patternWindmill;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    else if (block is IMyGasGenerator)
    {
        sumO2Gen[0]++; sumAll[0]++;
        percent = 100 / (float)block.GetInventory().MaxVolume * (float)block.GetInventory().CurrentVolume;
        sumO2Gen[1] += percent; sumAll[1] += percent;
        sumO2Gen[2] += (float)block.GetInventory().CurrentVolume;
        icon = iconIce;
        isMonoicon = true;
        pattern = patternGasgen;
        SetFrameColorAndValidBlock(block, ref isValidBlock);
    }
    return isValidBlock;
}
