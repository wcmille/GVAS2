Program() {
  GeneratePatterns();
  if (!Me.CustomName.EndsWith("[FSD]")) { Me.CustomName += " [FSD]";}
  cs = scrollpositions.GetUpperBound(0) + 1;
  ce = scrollpositions.GetUpperBound(1) + 1;
