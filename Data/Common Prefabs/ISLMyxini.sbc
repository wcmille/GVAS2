    break;
    case SmallBar:
      AddFrameSprite(pos, new Vector2(set_length,51.84f), false, Colors[frm]);
    break;
    default:
      for (int i = 0; i &lt; pattern.Count();) {
        AddTextureSprite("SquareSimple", pos+(pattern[i++]*scale), pattern[i++]*scale, Colors[frm], C_align);
      }
